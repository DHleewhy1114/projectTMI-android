<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DailyRecord">

    <include
        android:id="@+id/recorded_daily_drug_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/activity_daily_drug"
        />
    <include
        android:id="@+id/recorded_daily_meal_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/record_title"
        />

    <include
        android:id="@+id/recorded_daily_exercise_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/activity_daily_meal"
        />


</androidx.constraintlayout.widget.ConstraintLayout>